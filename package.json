{
  "name": "ecletron-porject",
  "version": "1.0.0",
  "description": "桌面应用",
  "main": "main.js",
  "bin": {
    "electron-start": "./script/vue-runner.js"
  },
  "scripts": {
    "start:electron": "electron .",
    "start:web": "webpack-dev-server --config webpack.config.js",
    "dev": "electron-start",
    "build:web": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:dir": "electron-builder --dir",
    "build": "cross-env NODE_ENV=production npm run build:web & electron-builder",
    "test": "npm run test",
    "check": "tsc --watch"
  },
  "build": {
    "productName": "my-app",
    "appId": "com.electron.ecletron-porject",
    "directories": {
      "output": "electron_dist"
    }
  },
  "author": "yuanchenglang",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.25.2",
    "element-plus": "^2.2.17",
    "pina": "^0.20.2204228",
    "ramda": "^0.28.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vue/compiler-sfc": "^3.2.38",
    "babel-loader": "^8.2.5",
    "cfonts": "^3.1.1",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^20.1.2",
    "electron-builder": "^23.3.3",
    "electron-debug": "^3.2.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "file-loader": "^6.2.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.4.2",
    "mini-css-extract-plugin": "^2.6.1",
    "npm": "^8.19.1",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^17.0.0",
    "vue-loader-plugin": "^1.3.0",
    "vue-plugin-upload": "^1.0.8",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "/src/**/*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
